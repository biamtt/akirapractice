{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6", "DOM"],
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "types": ["cypress", "node"],
  },
  "include": [
    "cypress/**/*.ts",
    "node_modules/cypress/types/**/*.d.ts"
  ],
  "exclude": [
    "cypress/e2e/**/*.js",
    "node_modules",
    "dist"
  ]

}