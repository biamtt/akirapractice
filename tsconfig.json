{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "types": ["cypress"]
  },
  "include": [
    "cypress/**/*.ts",
    "node_modules/cypress/types/mocha/index.d.ts",
    "node_modules/cypress/types/mocha/index.d.ts"
  ],

}